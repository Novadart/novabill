<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.novadart.novabill.frontend.client.i18n.I18N" field="i18n"></ui:with>
	
	<g:FlowPanel styleName="authDialog">
		<g:Label styleName="reloadMessage" text="{i18n.sessionExpiredError}"></g:Label>
		<g:Label styleName="reloadMessage" text="{i18n.sessionExpiredError2}"></g:Label>
		
		<g:Button ui:field="reloadPage" styleName="reloadButton submit action2-button" text="{i18n.reloadPage}"></g:Button>
	</g:FlowPanel>
	
</ui:UiBinder> 