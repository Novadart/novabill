<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:with type="com.novadart.novabill.frontend.client.i18n.I18N" field="i18n"></ui:with>
	
	<g:FlowPanel styleName="body">
		<g:Label styleName="title"></g:Label>
		
		<g:HorizontalPanel styleName="filterContainer">
			<g:TextBox ui:field="filter"></g:TextBox>
			<g:Image ui:field="clearFilter"></g:Image>
		</g:HorizontalPanel>
		<g:SimplePanel ui:field="listWrapper"></g:SimplePanel>
		
		<g:HorizontalPanel styleName="buttons">
			<g:Button ui:field="ok" enabled="false" text="{i18n.submit}"></g:Button>
			<g:Button ui:field="cancel" text="{i18n.cancel}"></g:Button>
		</g:HorizontalPanel>
	</g:FlowPanel>
</ui:UiBinder> 