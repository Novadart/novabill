<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:with type="com.novadart.novabill.frontend.client.i18n.I18N"
		field="i18n"></ui:with>

	<g:SimplePanel styleName="wrapper">
		<g:Grid styleName="grid">
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.name}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value" ui:field="name" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.surname}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value" ui:field="surname" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.email}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value" ui:field="email" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.mobile}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value"  ui:field="mobile"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.phone}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value"  ui:field="phone"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label styleName="label" text="{i18n.fax}" />
				</g:customCell>
				<g:customCell>
					<g:Label styleName="value" ui:field="fax" />
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:SimplePanel>

</ui:UiBinder> 