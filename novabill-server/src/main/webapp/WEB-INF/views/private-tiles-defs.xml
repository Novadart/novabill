<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

	<!-- LAYOUTS -->
	
	<definition name="private.layout" template="/WEB-INF/views/layouts/private/privateLayout.jsp">
	   <put-attribute name="novabill.version" value="${project.version}"></put-attribute>
	   <put-attribute name="css" value="private.${mvn.tiles.css.definition}" />
	   <put-attribute name="analytics" value="/WEB-INF/views/tiles/private/analytics.jsp" />
	   <put-attribute name="javascript" value="private.${mvn.tiles.javascript.definition}" />
	</definition>
	
	<definition name="private.hello.layout" template="/WEB-INF/views/layouts/private/privateHelloLayout.jsp">
       <put-attribute name="novabill.version" value="${project.version}"></put-attribute>
       <put-attribute name="css" value="private.${mvn.tiles.css.definition}" />
       <put-attribute name="analytics" value="/WEB-INF/views/tiles/private/analytics.jsp" />
       <put-attribute name="javascript" value="private.${mvn.tiles.javascript.definition}" />
    </definition>

    <!-- WRO4J HELPERS -->
    <definition name="private.minifiedJS" template="/WEB-INF/views/tiles/wro4j/private/minifiedJS.jsp" />
    <definition name="private.minifiedCSS" template="/WEB-INF/views/tiles/wro4j/private/minifiedCSS.jsp" />
    <definition name="private.expandedJS" template="/WEB-INF/views/tiles/wro4j/private/expandedJS.jsp" />
    <definition name="private.expandedCSS" template="/WEB-INF/views/tiles/wro4j/private/expandedCSS.jsp" />

	<!-- PAGES -->
	<definition name="private.dashboard" extends="private.layout">
		<put-attribute name="body" value="/WEB-INF/views/tiles/private/dashboardBody.jsp" />
		<put-attribute name="javascriptExtra" value="/WEB-INF/views/tiles/private/dashboardExtraJS.jsp" />
	</definition>
	
