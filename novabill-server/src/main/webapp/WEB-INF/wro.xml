<?xml version="1.0" encoding="UTF-8"?>
<groups xmlns="http://www.isdc.ro/wro">
    
    <group name="novabill-deps-base">
        <js>/novabill-deps/angular/angular.min.js</js>
        <js>/novabill-deps/angular/angular-route.min.js</js>
        <js>/novabill-deps/angular/angular-sanitize.min.js</js>
        <js>/novabill-deps/angular/angular-animate.min.js</js>
        <js>/novabill-deps/angular/angular-resource.min.js</js>
        <js>/novabill-deps/angular/i18n/angular-locale_it-it.js</js>
        <js>/novabill-deps/angular-translate.min.js</js>
        <js>/novabill-deps/angular-ui/ui-bootstrap-tpls-0.11.0.min.js</js>
        <js>/novabill-deps/ng-infinite-scroll.min.js</js>
    </group>
    
    <group name="novabill-deps-private">
        <js>/novabill-deps/angular-file-upload.min.js</js>
        <js>/novabill-deps/bignumber-1.3.0/bignumber.min.js</js>
        <js>/novabill-deps/stacktracejs-0.6.0/stacktrace.js</js>
    </group>

	<group name="novabill-private">
		<group-ref>novabill-deps-base</group-ref>
		<group-ref>novabill-deps-private</group-ref>
		<js>/novabill/private/scripts/*.js</js>
		<css>/novabill/private/css/*.css</css>
	</group>
	
	<group name="novabill-frontend">
        <group-ref>novabill-deps-base</group-ref>
        <js>/novabill/frontend/scripts/*.js</js>
        <css>/novabill/frontend/css/*.css</css>
    </group>
	

</groups>